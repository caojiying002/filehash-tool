#!/usr/bin/make -f

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

%:
	dh $@

override_dh_auto_build:
	$(MAKE) all

override_dh_auto_install:
	$(MAKE) install PREFIX=$(CURDIR)/debian/filehash/usr

override_dh_auto_clean:
	$(MAKE) clean

override_dh_auto_test:
	# Skip tests during packaging to avoid dependencies on test files